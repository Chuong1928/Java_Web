package bo;

import java.util.ArrayList;

import bean.loaibean;
import dao.loaidao;


public class loaibo  {
	loaidao s1= new loaidao();
	ArrayList<loaibean> ds;
	public ArrayList<loaibean> getloai() {
		try {
			ds=s1.getloai();
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		return ds;
	}
}
